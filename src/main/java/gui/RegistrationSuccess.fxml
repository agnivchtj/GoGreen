<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="successPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.RegistrationSuccessController">
    <children>
        <ImageView fitHeight="450.0" fitWidth="700.0" layoutX="-1.0" layoutY="-2.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../../files/gui/background.jpg" />
            </image>
        </ImageView>
        <Button fx:id="backToLogin" layoutX="248.0" layoutY="238.0" mnemonicParsing="false" onAction="#backToLogin" prefHeight="33.0" prefWidth="104.0" stylesheets="@style.css" text="Go back to login">
                <font>
                    <Font name="Segoe UI" size="12.0" />
                </font>
        </Button>
        <ImageView fitHeight="127.0" fitWidth="129.0" layoutX="14.0" layoutY="-2.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../../files/gui/goGreenlogo.png" />
            </image>
        </ImageView>
        <Label layoutX="72.0" layoutY="158.0" prefHeight="47.0" prefWidth="543.0" text="You have successfully registered yourself!" textFill="#fcf9f9">
            <font>
                <Font name="Agency FB" size="35.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
